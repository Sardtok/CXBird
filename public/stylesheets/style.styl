html 
  -ms-touch-action: none; /* Direct all pointer events to JavaScript code. */
  -webkit-user-select: none;
body
  text-align:center
  font: 18px "Lucida Grande", Helvetica, Arial, sans-serif
  background: #c8e8ea url('../img/bg3.png') top left repeat-x
a
  color: #00B7FF
.flybutton
  position:fixed
  top:0
  left:0
  bottom:0
  right:0
  background:red
  .header
    background: white
    opacity: 0.8
    text-align:center
    font-size: 20px
    color: white
    padding: 10px
	.text
      color: #222
      text-shadow: 0px 1px 1px #4d4d4d
img.player
  width:100%
  max-height:90%
  position:absolute
  left:0
  bottom:0
p.error
  color: red
a.btn
  text-align:center
  background: #ECECEC;
  border-radius: 15px;
  padding: 10px 20px;
  display: block;
  font-family: arial;
  font-weight: bold;
  color:#7f7f7f;
  text-decoration: none;
  text-shadow:0px 1px 0px #fff;
  border:1px solid #a7a7a7;
  width: 145px;
  margin:0px auto;
  margin-top:40px;
  box-shadow: 0px 2px 1px white inset, 0px -2px 8px white, 0px 2px 5px rgba(0, 0, 0, 0.1), 0px 8px 10px rgba(0, 0, 0, 0.1);
  -webkit-transition:box-shadow 0.5s;
a.btn i
  float: right;
  margin-top: 2px;

a.btn:hover
  box-shadow: 0px 2px 1px white inset, 0px -2px 20px white, 0px 2px 5px rgba(0, 0, 0, 0.1), 0px 8px 10px rgba(0, 0, 0, 0.1);

a.btn:active
  box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.5) inset, 0px -2px 20px white, 0px 1px 5px rgba(0, 0, 0, 0.1), 0px 2px 10px rgba(0, 0, 0, 0.1);
  background: -webkit-linear-gradient(top, #d1d1d1 0%,#ECECEC 100%);

a.burger
  background: rgba(200,200,255,0.5)
  border: 1px solid rgba(0,0,0,0.5)
  border-radius: 2px
  width:34px
  cursor: pointer
a.burger div
	border: 1px solid black
	height:3px
	border-radius: 3px
	background: black
	margin: 3px

.pull-left
  float: left

.highscores .list
  display: inline-block
  margin: 0 auto
  font-size: 20pt
.highscores .list
  margin: 5px
.highscores .list .me
  color: yellow
.list .col1
  width: 80px
  float: left
.list .col2
  width: 380px
  float: left

.view-animate-container
  margin: 15px
  position:absolute
  top:0
  bottom:0
  left:0
  right:0
  background: rgba(255,255,255,0.4)
  border:1px solid rgba(0,0,0,0.4)
  overflow:hidden

.view-animate
  padding: 15px
.view-animate.ng-enter,
.view-animate.ng-leave
  -webkit-transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.5s
  transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.5s
  display:block
  width:100%
  position:absolute
  top:0
  left:0
  right:0
  bottom:0
.view-animate.ng-enter
  left:100%
.view-animate.ng-enter.ng-enter-active
  left:0
.view-animate.ng-leave.ng-leave-active
  left:-100%
  color: red

td.number
  text-align:right

table.currentplayers
  margin:20px
  font-family: 'Lucida Console', Monaco, monospace

table.currentplayers th
  background-color: #0090db

table.currentplayers th, table.currentplayers td
  padding:10px

table.currentplayers,table.currentplayers th, table.currentplayers td
  border: 1px solid #19469D
  border-collapse:collapse

cxbird
  float:left
  margin:20px



.absolute
  position:absolute
.relative
  position:relative
.center
  margin: 0 auto
.fullsize
  width: 100%
  height: 100%
.ticket
  display:inline-block
  margin: 3px
.winner
  font-size: 30px
  color:yellow
  -webkit-animation-name: 'pulse_animation';
  -webkit-animation-duration: 2000ms;
  -webkit-transform-origin:70% 70%;
  -webkit-animation-iteration-count: infinite;
  -webkit-animation-timing-function: linear;
  -moz-text-shadow: 1px 0px 20px yellow;
  -webkit-text-shadow: 1px 0px 20px yellow;
  text-shadow: 1px 0px 20px yellow;
@-webkit-keyframes pulse_animation
  0% { font-size:30px; }
  50% { font-size:60px; }
  100% { font-size:30px; }
/*
  0% { -webkit-transform: scale(1); }
  50% { -webkit-transform: scale(2); }
  100% { -webkit-transform: scale(1); }
  */



.spin.ng-enter, .spin.ng-move
  display: block;

.spin.ng-enter.ng-enter-active,
.spin.ng-move.ng-move-active
	opacity:1;

.spin.ng-leave
  -webkit-animation:1s leave
  -moz-animation:1s leave
  -o-animation:1s leave
  animation:1s leave

@-webkit-keyframes enter {
    from {
        opacity: 0;
        height: 0px;
        top: -50px;
        left: -5px;
    }
    10% {
        left: 45px;
    }
    20% {
        top: 40px;
    }
    30% {
        left: -35px;
    }
    40% {
        top: -30px;
    }
    50% {
        left: 25px;
    }
    60% {
        top: 20px;
    }
    70% {
        left: -15px;
    }
    80% {
        top: -10px;
    }
    90% {
        left: 5px;
    }
    to {
        opacity: 1;
        height: 30px;
        left: 0px;
        top: 0px;
    }
}
